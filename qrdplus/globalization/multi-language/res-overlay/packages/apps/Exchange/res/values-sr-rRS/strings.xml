    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <string name="account_setup_options_mail_window_3days">"Три дана"</string>
    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <string name="account_setup_options_mail_window_1week">"Недељу дана"</string>
    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <string name="account_setup_options_mail_window_2weeks">"Две недеље"</string>
    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <string name="account_setup_options_mail_window_1month">"Месец дана"</string>
    <!-- A sync window length setting (i.e. load messages this far back) [CHAR LIMIT=25]  -->
    <string name="account_setup_options_mail_window_all">"Све"</string>

    <string name="account_manager_type_exchange" translatable="false">com.android.exchange</string>
    <string name="authority_email_provider" translatable="false">com.android.email.provider</string>

    <!-- Title for notification warning about an authentication error [CHAR LIMIT=40]  -->
    <string name="auth_error_notification_title">"Грешка при потврди аутентичности"</string>

    <!-- text for notification warning about an authentication error [CHAR LIMIT=60]  -->
    <string name="auth_error_notification_text">"Додирните да бисте изменили подешавања налога за <xliff:g id="ACCOUNT">%s</xliff:g>"</string>

    <!-- Message displayed when there is no application to handle the settings option. [CHAR LIMIT=NONE] -->
    <string name="email_settings_not_available">"Омогућите поново апликацију Имејл да бисте прегледали подешавања."</string>

