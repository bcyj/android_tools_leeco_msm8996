<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Spoken description to let the user know that when typing in a password, they can plug in a headset in to hear spoken descriptions of the keys they type. [CHAR LIMIT=NONE] -->
    <string name="spoken_use_headphones">"Подключите гарнитуру, чтобы услышать пароль."</string>

    <!-- Spoken description for the currently entered text -->
    <string name="spoken_current_text_is">"Введенный текст: %s."</string>
    <!-- Spoken description when there is no text entered -->
    <string name="spoken_no_text_entered">"Текст не введен."</string>

    <!-- Spoken description to let the user know what auto-correction will be performed when a key is pressed. An auto-correction replaces a single word with one or more words. -->
    <string name="spoken_auto_correct">"При нажатии клавиши <xliff:g id="KEY_NAME">%1$s</xliff:g> слово <xliff:g id="ORIGINAL_WORD">%2$s</xliff:g> будет исправлено на <xliff:g id="CORRECTED_WORD">%3$s</xliff:g>."</string>
    <!-- Spoken description used during obscured (e.g. password) entry to let the user know that auto-correction will be performed when a key is pressed. -->
    <string name="spoken_auto_correct_obscured">"Клавиша <xliff:g id="KEY_NAME">%1$s</xliff:g> выполняет автоисправление."</string>

    <!-- Spoken description for unknown keyboard keys. -->
    <string name="spoken_description_unknown">"Неизвестный символ."</string>
    <!-- Spoken description for the "Shift" keyboard key when "Shift" is off. -->
    <string name="spoken_description_shift">"Клавиша верхнего регистра."</string>
    <!-- Spoken description for the "Shift" keyboard key in symbols mode. -->
    <string name="spoken_description_symbols_shift">"Дополнительные символы."</string>
    <!-- Spoken description for the "Shift" keyboard key when "Shift" is on. -->
    <string name="spoken_description_shift_shifted">"Клавиша верхнего регистра."</string>
    <!-- Spoken description for the "Shift" keyboard key in 2nd symbols (a.k.a. symbols shift) mode. -->
    <string name="spoken_description_symbols_shift_shifted">"Символы."</string>
    <!-- Spoken description for the "Shift" keyboard key when "Caps lock" is on. -->
    <string name="spoken_description_caps_lock">"Клавиша верхнего регистра."</string>
    <!-- Spoken description for the "Delete" keyboard key. -->
    <string name="spoken_description_delete">"Удалить."</string>
    <!-- Spoken description for the "To Symbol" keyboard key. -->
    <string name="spoken_description_to_symbol">"Символы."</string>
    <!-- Spoken description for the "To Alpha" keyboard key. -->
    <string name="spoken_description_to_alpha">"Буквы."</string>
    <!-- Spoken description for the "To Numbers" keyboard key. -->
    <string name="spoken_description_to_numeric">"Цифры."</string>
    <!-- Spoken description for the "Settings" keyboard key. -->
    <string name="spoken_description_settings">"Настройки."</string>
    <!-- Spoken description for the "Tab" keyboard key. -->
    <string name="spoken_description_tab">"Клавиша Tab."</string>
    <!-- Spoken description for the "Space" keyboard key. -->
    <string name="spoken_description_space">"Пробел."</string>
    <!-- Spoken description for the "Mic" keyboard key. -->
    <string name="spoken_description_mic">"Голосовой ввод."</string>
    <!-- Spoken description for the "Emoji" keyboard key. -->
    <string name="spoken_description_emoji">"Смайлики."</string>
    <!-- Spoken description for the "Return" keyboard key. -->
    <string name="spoken_description_return">"Ввод."</string>
    <!-- Spoken description for the "Search" keyboard key. -->
    <string name="spoken_description_search">"Поиск."</string>
    <!-- Spoken description for the "U+2022" (BULLET) keyboard key. -->
    <string name="spoken_description_dot">"Маркер списка."</string>
    <!-- Spoken description for the "Switch language" keyboard key. -->
    <string name="spoken_description_language_switch">"Сменить язык."</string>
    <!-- Spoken description for the "Next" action keyboard key. -->
    <string name="spoken_description_action_next">"Далее."</string>
    <!-- Spoken description for the "Previous" action keyboard key. -->
    <string name="spoken_description_action_previous">"Назад."</string>

    <!-- Spoken feedback after turning "Shift" mode on. -->
    <string name="spoken_description_shiftmode_on">"Верхний регистр включен."</string>
    <!-- Spoken feedback after turning "Caps lock" mode on. -->
    <string name="spoken_description_shiftmode_locked">"Caps Lock включен."</string>
    <!-- Spoken feedback after changing to the symbols keyboard. -->
    <string name="spoken_description_mode_symbol">"Режим добавления символов."</string>
    <!-- Spoken feedback after changing to the 2nd symbols (a.k.a. symbols shift) keyboard. -->
    <string name="spoken_description_mode_symbol_shift">"Режим дополнительных символов."</string>
    <!-- Spoken feedback after changing to the alphanumeric keyboard. -->
    <string name="spoken_description_mode_alpha">"Режим ввода текста."</string>
    <!-- Spoken feedback after changing to the phone dialer keyboard. -->
    <string name="spoken_description_mode_phone">"Режим набора номера."</string>
    <!-- Spoken feedback after changing to the shifted phone dialer (symbols) keyboard. -->
    <string name="spoken_description_mode_phone_shift">"Режим телефонных символов."</string>

    <!-- Spoken feedback when the keyboard is hidden. -->
    <string name="announce_keyboard_hidden">"Клавиатура скрыта."</string>
    <!-- Spoken feedback when the keyboard mode changes. -->
    <string name="announce_keyboard_mode">"Включен режим <xliff:g id="KEYBOARD_MODE">%s</xliff:g>."</string>
    <!-- Description of the keyboard mode for entering dates. -->
    <string name="keyboard_mode_date">"ввода даты"</string>
    <!-- Description of the keyboard mode for entering dates and times. -->
    <string name="keyboard_mode_date_time">"ввода даты и времени"</string>
    <!-- Description of the keyboard mode for entering email addresses. -->
    <string name="keyboard_mode_email">"ввода адреса электронной почты"</string>
    <!-- Description of the keyboard mode for entering text messages. -->
    <string name="keyboard_mode_im">"ввода сообщения"</string>
    <!-- Description of the keyboard mode for entering numbers. -->
    <string name="keyboard_mode_number">"ввода цифр"</string>
    <!-- Description of the keyboard mode for entering phone numbers. -->
    <string name="keyboard_mode_phone">"набора номера"</string>
    <!-- Description of the keyboard mode for entering generic text. -->
    <string name="keyboard_mode_text">"ввода текста"</string>
    <!-- Description of the keyboard mode for entering times. -->
    <string name="keyboard_mode_time">"ввода времени"</string>
    <!-- Description of the keyboard mode for entering URLs. -->
    <string name="keyboard_mode_url">"ввода URL"</string>

    <!-- Description of the emoji category icon of Recents. -->
    <string name="spoken_descrption_emoji_category_recents">"Недавно использованные."</string>
    <!-- Description of the emoji category icon of People. -->
    <string name="spoken_descrption_emoji_category_people">"Люди."</string>
    <!-- Description of the emoji category icon of Objects. -->
    <string name="spoken_descrption_emoji_category_objects">"Объекты."</string>
    <!-- Description of the emoji category icon of Nature. -->
    <string name="spoken_descrption_emoji_category_nature">"Природа."</string>
    <!-- Description of the emoji category icon of Places. -->
    <string name="spoken_descrption_emoji_category_places">"Места."</string>
    <!-- Description of the emoji category icon of Symbols. -->
    <string name="spoken_descrption_emoji_category_symbols">"Символы."</string>
    <!-- Description of the emoji category icon of Emoticons. -->
    <string name="spoken_descrption_emoji_category_emoticons">"Смайлики."</string>

    <!-- Description of an upper case letter of LOWER_LETTER. -->
    <string name="spoken_description_upper_case">"Заглавная <xliff:g id="LOWER_LETTER">%s</xliff:g>."</string>
    <!-- Spoken description for Unicode code point U+0049: "I" LATIN CAPITAL LETTER I
         Note that depending on locale, the lower-case of this letter is U+0069 or U+0131. -->
    <string name="spoken_letter_0049">"Заглавная латинская I."</string>
    <!-- Spoken description for Unicode code point U+0130: "İ" LATIN CAPITAL LETTER I WITH DOT ABOVE
         Note that depending on locale, the lower-case of this letter is U+0069 or U+0131. -->
    <string name="spoken_letter_0130">"Заглавная латинская I с точкой сверху."</string>
    <!-- Spoken description for unknown symbol code point. -->
    <string name="spoken_symbol_unknown">"Неизвестный символ."</string>
    <!-- Spoken description for unknown emoji code point. -->
    <string name="spoken_emoji_unknown">"Неизвестный смайлик."</string>

    <!-- Spoken descriptions when opening a more keys keyboard that has alternative characters. -->
    <string name="spoken_open_more_keys_keyboard">"Доступны дополнительные символы."</string>
    <!-- Spoken descriptions when closing a more keys keyboard that has alternative characters. -->
    <string name="spoken_close_more_keys_keyboard">"Дополнительные символы скрыты."</string>

    <!-- Spoken descriptions when opening a more suggestions panel that has alternative suggested words. -->
    <string name="spoken_open_more_suggestions">"Доступны дополнительные подсказки."</string>
    <!-- Spoken descriptions when closing a more suggestions panel that has alternative suggested words. -->
    <string name="spoken_close_more_suggestions">"Дополнительные подсказки скрыты."</string>
</resources>
